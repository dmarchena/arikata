{
  "name": "uoc-tfg",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "dmarchena",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config webpack/client.dev.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/client.prod.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server.config.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config webpack/server.config.js",
    "clean": "rm -rf ./dist",
    "predev": "npm run clean && npm run build:server:dev",
    "dev": "node ./dist/server.js",
    "prestart": "npm run build",
    "start": "node ./dist/server.js",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint:js": "eslint --ext .js,.vue --fix .",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "eslint-plugin-vue-a11y": "^0.0.31",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-stylelint": "^0.4.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "terser-webpack-plugin": "^2.3.5",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "core-js": "3.6.4",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.6",
    "regenerator-runtime": "^0.13.3",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6"
  }
}
