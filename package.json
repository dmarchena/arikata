{
  "name": "uoc-tfg",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "dmarchena",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:dev": "npm run clean && npm run build:client:dev && npm run build:server:dev",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.client-prod.js",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.client-dev.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.server.js",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config webpack/webpack.config.server.js",
    "clean": "rm -rf ./dist",
    "db:migrate": "cross-env NODE_ENV=localhost npx sequelize-cli db:migrate",
    "db:migrate:undo": "cross-env NODE_ENV=localhost npx sequelize-cli db:migrate:undo:all",
    "db:seed": "cross-env NODE_ENV=localhost npx sequelize-cli db:seed:all",
    "db:seed:undo": "cross-env NODE_ENV=localhost npx sequelize-cli db:seed:undo:all",
    "predev": "npm run clean && npm run build:server:dev",
    "dev": "node ./dist/server.js",
    "prestart": "npm run build",
    "start": "node ./dist/server.js",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"**/*.css\" --fix",
    "lint:js": "eslint --ext .js,.vue --fix .",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-a11y": "^0.0.31",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "prettier-stylelint": "^0.4.2",
    "sequelize-cli": "^5.5.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "terser-webpack-plugin": "^2.3.6",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "core-js": "3.6.5",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "express-static-gzip": "^2.0.6",
    "faker": "^4.1.0",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.3",
    "ramda": "^0.27.0",
    "regenerator-runtime": "^0.13.5",
    "sequelize": "^5.21.7",
    "uuid": "^7.0.3",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6"
  }
}
